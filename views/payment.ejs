      <input id="payment-amount" type="number" placeholder="Amount" name="Amount" required><span class="dollar-symbol"><i class="fas fa-dollar-sign"></i></span>
      <input id="payment-note" type="text" placeholder="Note" name="Note">
      <div id="card-container"></div>
      <button id="card-button" class="button-credit-card" type="button">Pay</button>
      <!-- <button id="sq-creditcard" class="button-credit-card" onclick="onGetCardNonce(event)">Pay Now</button> -->
      <button class="apple-pay-button apple-pay-button-black" id="sq-apple-pay" onclick="createPaymentRequest(event)"></button>
      <!-- <div id="sq-apple-pay-label" class="wallet-not-enabled">Apple Pay not enabled</div> -->
      <div id="payment-status-container"></div>
